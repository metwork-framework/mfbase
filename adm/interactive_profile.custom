{% extends "interactive_profile" %}

{% block custom %}
if test "${MFBASE_CURRENT_PLUGIN_NAME:-}" != ""; then
    alias base="psql -U plugin_${MFBASE_CURRENT_PLUGIN_NAME} -h ${MFMODULE_RUNTIME_HOME}/var -p ${MFBASE_POSTGRESQL_PORT} plugin_${MFBASE_CURRENT_PLUGIN_NAME}"
else
    alias base="psql -U ${MFBASE_POSTGRESQL_USERNAME} -h ${MFMODULE_RUNTIME_HOME}/var -p ${MFBASE_POSTGRESQL_PORT} ${MFBASE_POSTGRESQL_DBNAME}"
fi
{% endblock %}
